#include <iostream>
#include <cutlass/matrix.h>

// Define matrix dimensions
const int M = 3;
const int N = 3;

int main() {
    // Define matrix type
    using MatrixType = cutlass::Matrix<float, M, N>;

    // Initialize a 3x3 matrix with zeros
    MatrixType matrix;

    // Print the matrix
    std::cout << "Initialized matrix:" << std::endl;
    for (int i = 0; i < M; ++i) {
        for (int j = 0; j < N; ++j) {
            std::cout << matrix.at(i, j) << " ";
        }
        std::cout << std::endl;
    }

    // Modify an element of the matrix
    matrix.at(1, 1) = 1.0f;

    // Print the modified matrix
    std::cout << "Modified matrix:" << std::endl;
    for (int i = 0; i < M; ++i) {
        for (int j = 0; j < N; ++j) {
            std::cout << matrix.at(i, j) << " ";
        }
        std::cout << std::endl;
    }

    return 0;
}

